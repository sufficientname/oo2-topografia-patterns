Object subclass: #Topography	instanceVariableNames: ''	classVariableNames: ''	package: 'TP3-3-Topografia'!!Topography methodsFor: 'testing' stamp: 'sufficientname 6/1/2021 11:45'!isLandTopography	^ false! !!Topography methodsFor: 'testing' stamp: 'sufficientname 6/1/2021 11:46'!isWaterTopography	^ false! !!Topography methodsFor: 'testing' stamp: 'sufficientname 6/1/2021 11:22'!isEqualTo: aTopography	self subclassResponsibility! !!Topography methodsFor: 'testing' stamp: 'sufficientname 6/1/2021 11:46'!isMixedTopography	^ false! !!Topography methodsFor: 'accessing' stamp: 'sufficientname 6/1/2021 02:35'!waterRatio	self subclassResponsibility ! !!Topography methodsFor: 'accessing' stamp: 'sufficientname 6/1/2021 02:35'!landRatio	self subclassResponsibility ! !Topography subclass: #CompositeTopography	instanceVariableNames: 'children'	classVariableNames: ''	package: 'TP3-3-Topografia'!!CompositeTopography methodsFor: 'private' stamp: 'sufficientname 6/2/2021 09:13'!ratioOf: aNumber	^ aNumber / children size! !!CompositeTopography methodsFor: 'private' stamp: 'sufficientname 6/2/2021 09:13'!addTopography: aTopography	children add: aTopography! !!CompositeTopography methodsFor: 'initialization' stamp: 'sufficientname 6/2/2021 09:13'!initialize	super initialize.	children := OrderedCollection new! !!CompositeTopography methodsFor: 'accessing' stamp: 'sufficientname 6/2/2021 09:13'!waterRatio	^ self		ratioOf: (children sum: [ :aTopography | aTopography waterRatio ])! !!CompositeTopography methodsFor: 'accessing' stamp: 'sufficientname 6/2/2021 09:13'!getTopographyAt: aNumber	^ children at: aNumber! !!CompositeTopography methodsFor: 'accessing' stamp: 'sufficientname 6/2/2021 09:13'!landRatio	^ self		ratioOf: (children sum: [ :aTopography | aTopography landRatio ])! !!CompositeTopography methodsFor: 'testing' stamp: 'sufficientname 6/1/2021 11:47'!isMixedTopography	^ true! !!CompositeTopography methodsFor: 'testing' stamp: 'sufficientname 6/2/2021 09:13'!isEqualTo: aTopography	aTopography isMixedTopography		ifFalse: [ ^ false ].	^ (1 to: children size)		allSatisfy: [ :index | 			(self getTopographyAt: index)				isEqualTo: (aTopography getTopographyAt: index) ]! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!CompositeTopography class	instanceVariableNames: ''!!CompositeTopography class methodsFor: 'instance creation' stamp: 'sufficientname 6/2/2021 09:27'!with: aTopography1 with: aTopography2 with: aTopography3 with: aTopography4	^ self new		addTopography: aTopography1;		addTopography: aTopography2;		addTopography: aTopography3;		addTopography: aTopography4;		yourself! !Topography subclass: #LeafTopography	instanceVariableNames: 'landRatio waterRatio'	classVariableNames: ''	package: 'TP3-3-Topografia'!!LeafTopography methodsFor: 'accessing' stamp: 'sufficientname 6/1/2021 11:24'!waterRatio	^ waterRatio! !!LeafTopography methodsFor: 'accessing' stamp: 'sufficientname 6/1/2021 11:24'!landRatio	^ landRatio! !LeafTopography subclass: #LandTopography	instanceVariableNames: ''	classVariableNames: ''	package: 'TP3-3-Topografia'!!LandTopography methodsFor: 'initialization' stamp: 'sufficientname 6/1/2021 11:25'!initialize	super initialize.	landRatio := 1.	waterRatio := 0! !!LandTopography methodsFor: 'testing' stamp: 'sufficientname 6/1/2021 11:53'!isEqualTo: aTopography	^ aTopography isLandTopography ! !!LandTopography methodsFor: 'testing' stamp: 'sufficientname 6/1/2021 11:51'!isLandTopography	^ true! !LeafTopography subclass: #WaterTopography	instanceVariableNames: ''	classVariableNames: ''	package: 'TP3-3-Topografia'!!WaterTopography methodsFor: 'initialization' stamp: 'sufficientname 6/1/2021 11:26'!initialize	super initialize.	landRatio := 0.	waterRatio := 1! !!WaterTopography methodsFor: 'testing' stamp: 'sufficientname 6/1/2021 11:51'!isWaterTopography	^ true! !!WaterTopography methodsFor: 'testing' stamp: 'sufficientname 6/1/2021 11:53'!isEqualTo: aTopography	^ aTopography isWaterTopography ! !